html
head
  title SpotFax
  meta(charset='utf-8')
  link(rel='stylesheet',  href="/bower_components/bootstrap/dist/css/bootstrap.min.css")
  link(rel='stylesheet',  href="/css/styles.css")
  
body
  div(class="container")
    div(name="location" id="location")
      div(class="row row-border")
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          <span class="fa fa-map-marker" aria-hidden="true"></span> City
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          div(id="city") ...
    div(name="environment" id="environment")
      div(class="row row-border aicuz")
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          <span class="fa fa-plane" aria-hidden="true"></span> AICUZ Noise Level
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          div(id="aicuz") ...
      div(class="row row-border")
        div(id="AICUZ0" class="col-md-4 col-lg-4 _0")
          t 0
        div(class="col-xs-3 col-sm-3 col-md-1 col-lg-1 _50")
          t 50
        div(class="col-xs-3 col-sm-3 col-md-1 col-lg-1 _55")
          t 55
        div(class="col-xs-3 col-sm-3 col-md-1 col-lg-1 _60")
          t 60
        div(class="col-xs-3 col-sm-3 col-md-1 col-lg-1 _65")
          t 65
        div(class="col-xs-3 col-sm-3 col-md-1 col-lg-1 _70")
          t 70
        div(class="col-xs-3 col-sm-3 col-md-1 col-lg-1 _75")
          t 75
        div(class="col-xs-3 col-sm-3 col-md-1 col-lg-1 _80")
          t 80
        div(class="col-xs-3 col-sm-3 col-md-1 col-lg-1 _85")
          t 85
      div(class="row row-border flood")
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          <span class="fa fa-life-ring" aria-hidden="true"></span> Flood Zone
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          div(id="flood") ...(this can take some time)
    div(name="safety" id="safety")
      div(class="row row-border ems")
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          <span class="fa fa-ambulance" aria-hidden="true"></span> EMS Response
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          div(id="ems-response-avg") ...
      div(class="row row-border police")
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          <span class="fa fa-shield" aria-hidden="true"></span> Police Response
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          div(id="police-response-avg") ...
      div(class="row row-border police")
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          <span class="fa fa-shield" aria-hidden="true"></span> Police Service Calls <a href="" title="Non-emergency">*</a>
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          div(id="police-calls") ...
      div(class="row row-border police")
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          <span class="fa fa-shield" aria-hidden="true"></span> Police Incidents <a href="" title="Emergency, possible crime">*</a>
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          div(id="police-incidents") ...
      div(class="row row-border fire")
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          <span class="fa fa-fire" aria-hidden="true"></span> Closest Public Hydrant
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          div(id="closest-hydrant") ...
    div(name="neighborhood" id="neighborhood")
      div(class="row row-border schools")
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          <span class="fa fa-graduation-cap" aria-hidden="true"></span> Nearby Schools
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          p(id="school-location-all")
      div(class="row row-border schools")
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          p Elementary
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          p(id="school-location-elementary")
      div(class="row row-border schools")
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          p Middle
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          p(id="school-location-middle")
      div(class="row row-border schools")
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          p High
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          p(id="school-location-high")
      div(class="row row-border schools")
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          <span class="fa fa-graduation-cap" aria-hidden="true"></span> Zoned Schools
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          p(id="school-zone-all")
      div(class="row row-border schools")
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          p Elementary
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          p(id="school-zone-elementary")
      div(class="row row-border schools")
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          p Middle
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          p(id="school-zone-middle")
      div(class="row row-border schools")
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          p High
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          p(id="school-zone-high")
      div(class="row row-border parks")
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          <span class="fa fa-tree" aria-hidden="true"></span> Parks within 1 mile
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          div(id="parks") ...
      div(class="row row-border parks")
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          <span class="fa fa-tree" aria-hidden="true"></span> Closest Park
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          div(id="closest-park") ...
      div(class="row row-border libraries")
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          <span class="fa fa-book" aria-hidden="true"></span> Closest Library
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          div(id="closest-library") ...
      div(class="row row-border recCenters")
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          <span class="fa fa-soccer-ball-o" aria-hidden="true"></span> Closest Recreation Center
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          div(id="closest-recCenter") ...
      div(class="row row-border neighborhoods")
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          <span class="fa fa-home" aria-hidden="true"></span> Nearby Neighborhoods
        div(class="col-xs-12 col-sm-12 col-md-6 col-lg-6")
          div(id="nearby-neighborhoods") ...
  div(id="invisibleMap")
  input(type="hidden" id="locations" value=""+locations)
  input(type="hidden" id="searchPosition" value=""+position)
  link(rel='stylesheet',  href="/bower_components/font-awesome/css/font-awesome.min.css")
  script(src="//d3js.org/d3.v3.min.js")
  script(src="//d3js.org/d3-collection.v1.min.js")
  script(src="//d3js.org/d3-dispatch.v1.min.js")
  script(src="//d3js.org/d3-dsv.v1.min.js")
  script(src="//d3js.org/d3-request.v1.min.js")
  script(src="//d3js.org/d3-array.v1.min.js")
  script(src="//d3js.org/d3-geo.v1.min.js")
  script(src="/bower_components/D.js/lib/D.min.js")
  script(src="/bower_components/jquery/dist/jquery.min.js")
  script(src="/bower_components/bootstrap/dist/js/bootstrap.min.js")
  script(src="/bower_components/jquery-ui/jquery-ui.min.js")
  link(rel="stylesheet"     
      href="/bower_components/jquery-ui/themes/smoothness/jquery-ui.css")
  link(rel="stylesheet"     
      href="https://unpkg.com/leaflet@1.1.0/dist/leaflet.css",
      integrity="sha512-wcw6ts8Anuw10Mzh9Ytw4pylW8+NAD4ch3lqm9lzAsTxg0GFeJgoAtxuCLREZSC5lUXdVyo/7yfsqFjQ4S+aKw==",
      crossorigin="")
  script(src="https://unpkg.com/leaflet@1.1.0/dist/leaflet.js",
  integrity="sha512-mNqn2Wg7tSToJhvHcqfzLMU6J4mkOImSPTxVZAdo+lcPlk+GhZmYgACEe0x35K7YzW1zJ7XyJV/TT1MrdXvMcA==",
  crossorigin="")
  script(src="https://unpkg.com/esri-leaflet@2.1.0/dist/esri-leaflet.js",
  integrity="sha512-Tojl3UMd387f6DdAJlo+fKfJZiP55fYT+6Y58yKbHydnueOdSFOxrgLPuUxm7VW1szEt3hZVwv3V2sSUCuT35w==",
  crossorigin="")
  script(src="/js/geo.js")
  script(src="/js/google.js")
      
